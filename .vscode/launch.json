{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "启动程序",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\server\\src\\module\\main\\main.controller.ts",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        },
        {
            "type": "node-terminal",
            "name": "运行脚本: start",
            "request": "launch",
            "command": "yarn start:dev",
            "cwd": "${workspaceFolder}\\server"
        }
    ]
}